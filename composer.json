{
    "name": "makinacorpus/api-generator",
    "description": "Generates client side API interface (TypeScript for example) code from your PHP classes.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Makina Corpus",
            "homepage": "https://makina-corpus.com/"
        },
        {
            "name": "Pierre RINEAU",
            "email" : "pierre.rineau@makina-corpus.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "psr/log": "^2.0|^3.0",
        "symfony/uid": "^7.0"
    },
    "require-dev": {
        "doctrine/orm": "^3.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.2",
        "ramsey/uuid": "^4.7",
        "symfony/config": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/yaml": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "MakinaCorpus\\ApiGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
             "MakinaCorpus\\ApiGenerator\\Tests\\": "tests/"
        }
    }
}
